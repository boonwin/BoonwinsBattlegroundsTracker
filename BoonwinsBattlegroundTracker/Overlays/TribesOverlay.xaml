<UserControl x:Class="BoonwinsBattlegroundTracker.TribesOverlay"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BoonwinsBattlegroundTracker"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" VerticalAlignment="Center" HorizontalAlignment="Center">
    <Grid x:Name="panel" HorizontalAlignment="Center" VerticalAlignment="Center" >
        <Image x:Name="imgTribes" Stretch="Fill" Width="150" Height="150"></Image>
    </Grid>
</UserControl>
