<UserControl x:Class="BoonwinsBattlegroundTracker.ConsoleOverlay"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BoonwinsBattlegroundTracker"
             mc:Ignorable="d" Height="200" Width="500">
     
    
    <Grid>
        <TextBox x:Name="tbConsoleText"  FontSize="10" Text="Console:" TextWrapping="Wrap" Foreground="White" Margin="5,5,5,5" Opacity=".70" Background="#FF212121"></TextBox>
    </Grid>
</UserControl>
